<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:s="library://ns.adobe.com/flex/spark"
		xmlns:fx="http://ns.adobe.com/mxml/2009"
		width="100%" height="100%">
	<fx:Metadata>[HostComponent("spark.components.Button")]</fx:Metadata>
	<s:states>
		<s:State name="up"/>
		<s:State name="over"/>
		<s:State name="down"/>
		<s:State name="disabled"/>
	</s:states>
	<!--<s:Ellipse x="0.5" y="0.5" width="51" height="51">-->
	<s:Rect top="0" left="0" right="0" bottom="0" >
		<s:fill>
			<s:SolidColor color="0xffffff" />
		</s:fill>
	</s:Rect>
	
	<s:Rect left="0" right="0" top="0" bottom="0" >
		<s:stroke>
			<s:SolidColorStroke caps="none" color="#1889C8" joints="miter" miterLimit="4" weight="1"
								color.down="#216289"
								color.over="#067DC6"
								color.up="#009CFF"/>
		</s:stroke>
		<s:fill>
			<s:SolidColor color.disabled="#CCCCCC"
						  color.down="#216289"
						  color.over="#067DC6"
						  color.up="#009CFF"/>
		</s:fill>
	<!--</s:Ellipse>-->
	</s:Rect>
	<s:RichText id="labelDisplay" color="#FFFFFF"  fontSize="18" fontWeight="bold"
				tabStops="S0" whiteSpaceCollapse="preserve"
				horizontalCenter="0" verticalCenter="0"/>
</s:Skin>
